{
  "name": "Odonata",
  "version": "1.0.0-beta1",
  "description": "Chassis with React, Redux, React-Router using Typescript",
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:6266a9ec/odonata.git"
  },
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --inline --progress --port 8080",
    "clean": "rimraf dist",
    "test": "jest",
    "build": "webpack",
    "lint": "tslint -c tslint.json 'src/**/*.ts*' -e 'node_modules/**/*'",
    "lint:fix": "tslint --fix -c tslint.json 'src/**/*.ts*' -e 'node_modules/**/*'"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-redux": "5.0.2",
    "react-router": "3.0.2",
    "react-router-redux": "4.0.7",
    "redux": "3.6.0",
    "redux-thunk": "2.2.0"
  },
  "devDependencies": {
    "@types/node": "7.0.5",
    "@types/jest": "18.1.1",
    "@types/react": "15.0.6",
    "@types/react-dom": "0.14.22",
    "@types/react-redux": "4.4.36",
    "@types/react-router": "3.0.0",
    "@types/react-router-redux": "4.0.39",
    "@types/react-test-renderer": "15.4.2",
    "@types/redux": "3.6.0",
    "@types/redux-thunk": "2.1.0",
    "awesome-typescript-loader": "3.0.0-beta.18",
    "css-loader": "0.26.1",
    "css-raw-loader": "0.1.1",
    "extract-text-webpack-plugin": "2.0.0-rc.2",
    "html-webpack-plugin": "2.28.0",
    "jest": "18.1.0",
    "jest-cli": "18.1.0",
    "raw-loader": "0.5.1",
    "react-test-renderer": "15.4.2",
    "rimraf": "2.5.4",
    "script-loader": "0.7.0",
    "source-map-loader": "0.1.6",
    "style-loader": "0.13.1",
    "ts-jest": "18.0.3",
    "tslint-eslint-rules": "3.2.3",
    "tslint-react": "2.3.0",
    "typescript": "2.1.5",
    "webpack": "2.2.1",
    "webpack-dev-middleware": "1.10.0",
    "webpack-dev-server": "1.16.3",
    "webpack-hot-middleware": "2.16.1",
    "webpack-merge": "2.6.1"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "collectCoverage": true
  }
}
